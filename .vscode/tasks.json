{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "${workspaceFolder}/main.c",
                "${workspaceFolder}/atc/atc_fd.c",
                "${workspaceFolder}/atc/atc.c",
                "-g3",
				"-L${workspaceFolder}",
				"-lft",
                "-o",
				"debug.out"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}